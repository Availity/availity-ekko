  },
  "keywords": [
    "mock",
    "api",
    "proxy",
    "rest",
    "availity"
  ],
  "author": "Robert McGuinness <robert.mcguinness.iii@gmail.com> (http://robmcguinness.com/)",
  "contributors": [
    {
      "name": "Robert McGuinness",
      "email": "rob.mcguinness@availity.com"
    },
    {
      "name": "Kasey Powers",
      "email": "kasey.powers@availity.com"
    },
    {
      "name": "Danny Noler",
      "email": "danny.noler@availity.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-busboy": "0.0.2",
    "cors": "2.8.1",
    "dateformat": "1.0.12",
    "errorhandler": "1.4.3",
    "express": "4.14.0",
