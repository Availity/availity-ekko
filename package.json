    "yargs": "6.0.0"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "chai": "3.5.0",
    "cross-env": "3.0.0",
    "eslint": "3.7.0",
    "eslint-config-availity": "2.0.0-beta.11",
